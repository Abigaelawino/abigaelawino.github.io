ploy cycle to confirm:\n * - HTTP 200 responses\n * - Expected content is present\n * - No broken paths\n * \n * Usage: node scripts/smoke-test.mjs [base-url]\n * Default base URL: http://localhost:3000 (for local testing)\n * For production: node scripts/smoke-test.mjs https://abigaelawino.github.io\n */\n\nimport { createHash } from 'crypto';\nimport { setTimeout } from 'timers/promises';\n\nconst baseUrl = process.argv[2] || 'http://localhost:3000';\n\n// Key endpoints to test\nconst endpoints = [\n  { path: '/', expectedContent: ['Abigael Awino', 'Data science'] },\n  { path: '/about/', expectedContent: ['About', 'Toolkit'] },\n  { path: '/projects/', expectedContent: ['Projects'] },\n  { path: '/blog/', expectedContent: ['Blog'] },\n  { path: '/contact/', expectedContent: ['Contact', 'form'] },\n  { path: '/resume/', expectedContent: ['Resume'] },\n  { path: '/contact/thanks/', expectedContent: ['Thank you'] },\n];\n\n// API endpoints to test\nconst apiEndpoints = [\n  { path: '/api/webhooks/build', method: 'POST', skipContentCheck: true }, // webhook, may not return 200 for GET\n  { path: '/api/deployments', expectedContent: ['deployments'] },\n  { path: '/api/sessions', expectedContent: ['sessions'] },\n  { path: '/api/optimize', method: 'POST', skipContentCheck: true }, // POST endpoint\n  { path: '/api/rate-limit', expectedContent: ['rate'] },\n];\n\n// Static assets to test\nconst staticAssets = [\n  '/assets/og.png',\n  '/robots.txt',\n  '/sitemap.xml',\n];\n\n// Error log\nconst errors = [];\nconst warnings = [];\n\n// Simple fetch function with timeout\nasync function fetchWithTimeout(url, options = {}) {\n  const controller = new AbortController();\n  const timeoutId = setTimeout(() => controller.abort(), 10000); // 10 second timeout\n  \n  try {\n    const response = await fetch(url, {\n      ...options,\n      signal: controller.signal\n    });\n    clearTimeout(timeoutId);\n    return response;\n  } catch (error) {\n    clearTimeout(timeoutId);\n   