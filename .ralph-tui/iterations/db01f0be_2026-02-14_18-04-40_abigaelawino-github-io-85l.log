lts);\n118:     }\n119: \n120:     // Calculate compression ratio\n121:     if (results.originalSize > 0) {\n122:       results.compressionRatio = (\n123:         ((results.originalSize - results.optimizedSize) / results.originalSize) *\n124:         100\n125:       ).toFixed(2);\n126:     }\n127: \n128:     return results;\n129:   } catch (error) {\n130:     console.error('Optimization failed:', error);\n131:     throw error;\n132:   }\n133: }\n134: \n135: // Process directory recursively\n136: async function processDirectory(inputPath, outputPath, results) {\n137:   const items = fs.readdirSync(inputPath);\n138: \n139:   for (const item of items) {\n140:     const inputItemPath = path.join(inputPath, item);\n141:     const outputItemPath = path.join(outputPath, item);\n142: \n143:     if (fs.statSync(inputItemPath).isDirectory()) {\n144:       fs.mkdirSync(outputItemPath, { recursive: true });\n145:       await processDirectory(inputItemPath, outputItemPath, results);\n146:     } else {\n147:       await processFile(inputItemPath, outputItemPath, results);\n148:     }\n149:   }\n150: }\n151: \n152: // Process individual file\n153: async function processFile(inputPath, outputPath, results) {\n154:   const ext = path.extname(inputPath).toLowerCase();\n155:   const stats = fs.statSync(inputPath);\n156:   const originalSize = stats.size;\n157: \n158:   results.originalSize += originalSize;\n159:   results.filesProcessed++;\n160: \n161:   let optimizedSize = originalSize;\n162:   const optimization = {\n163:     file: path.basename(inputPath),\n164:     originalSize,\n165:     optimizedSize,\n166:     techniques: [],\n167:   };\n168: \n169:   try {\n170:     // Ensure output directory exists\n171:     fs.mkdirSync(path.dirname(outputPath), { recursive: true });\n172: \n173:     switch (ext) {\n174:       case '.css':\n175:         optimizedSize = await optimizeCSS(inputPath, outputPath, optimization);\n176:         break;\n177:       case '.js':\n178:         optimizedSize = await optimizeJS(inputPath, outputPath, optimization);\n179:         break;\n180:       case '.html':\n181:         optimizedSize = await optimizeHTML(inputPath, outputPath, optimization);\n182:         break;\n183:       case '.png':\n184:       case '.jpg':\n185:       case '.jpeg':\n186:       case '.svg':\n187:         optimizedSize = await optimizeImage(inputPath, outputPath, optimization);\n188:         break;\n189:       default:\n190:         // Copy file as-is\n191:         fs.copyFileSync(inputPath, outputPath);\n192:         optimization.techniques.push('copied');\n193:     }\n194: \n195:     results.optimizedSize += optimizedSize;\n196:     results.optimizations.push(optimization);\n197:   } catch (error) {\n198:     console.error(`Failed to optimize ${inputPath}:`, error);\n199:     // Copy original file if optimization fails\n200:     fs.copyFileSync(inputPath, outputPath);\n201:     optimizedSize = originalSize;\n202:     optimization.techniques.push('fallback');\n203:     results.optimizedSize += optimizedSize;\n204:   }\n205: }\n206: \n207: // Optimize CSS files\n208: async function optimizeCSS(inputPath, outputPath, optimization) {\n209:   let content = fs.readFileSync(inputPath, 'utf8');\n210: \n211:   // Minify CSS\n212:   content = content\n213:     .replace(/\\/\\*[\\s\\S]*?\\*\\//g, '') // Remove comments\n214:     .replace(/\\s+/g, ' ') // Collapse whitespace\n215:     .replace(/;\\s*}/g, '}') // Remove unnecessary semicolons\n216:     .replace(/\\s*{\\s*/g, '{') // Remove spaces around braces\n217:     .replace(/\\s*}\\s*/g, '}')\n218:     .replace(/\\s*;\\s*/g, ';') // Remove spaces around semicolons\n219:     .replace(/\\s*:\\s*/g, ':') // Remove spaces around colons\n220:     .replace(/\\s*,\\s*/g, ',') // Remove spaces around commas\n221:     .trim();\n222: \n223:   fs.writeFileSync(outputPath, content, 'utf8');\n224:   optimization.techniques.push('css-minify');\n225: \n226:   return Buffer.byteLength(content, 'utf8');\n227: }\n2