  run: npm install -g netlify-cli\n54: \n55:       - name: Authenticate with Netlify\n56:         if: ${{ secrets.NETLIFY_AUTH_TOKEN != '' }}\n57:         run: netlify login --token ${{ secrets.NETLIFY_AUTH_TOKEN }}\n58:         env:\n59:           NETLIFY_AUTH_TOKEN: ${{ secrets.NETLIFY_AUTH_TOKEN }}\n60: \n61:       - name: Run weekly log analysis\n62:         run: node scripts/netlify-log-analyzer.mjs\n63: \n64:       - name: Run performance metrics tracking\n65:         run: node scripts/netlify-performance-tracker.mjs\n66: \n67:       - name: Run monthly trend analysis\n68:         if: ${{ github.event.inputs.analysis_type == 'monthly' || github.event.inputs.analysis_type == 'comprehensive' }}\n69:         run: node scripts/netlify-monthly-trends.mjs || true\n70: \n71:       - name: Upload analysis artifacts\n72:         uses: actions/upload-artifact@v4\n73:         with:\n74:           name: netlify-performance-analysis-${{ github.run_number }}\n75:           path: .netlify-reports/\n76:           retention-days: 90\n77: \n78:       - name: Generate performance summary\n79:         id: performance-summary\n80:         run: |\n81:           if [ -f \".netlify-reports/performance-trends.json\" ]; then\n82:             HEALTH_SCORE=$(node -e \"\n83:               const data = JSON.parse(require('fs').readFileSync('.netlify-reports/performance-trends.json', 'utf8'));\n84:               console.log(data.trends.health_score || 0);\n85:             \")\n86:             SUCCESS_RATE=$(node -e \"\n87:               const data = JSON.parse(require('fs').readFileSync('.netlify-reports/performance-trends.json', 'utf8'));\n88:               console.log(data.trends.build_performance?.success_rate?.toFixed(1) || 0);\n89:             \")\n90:             AVG_BUILD_TIME=$(node -e \"\n91:               const data = JSON.parse(require('fs').readFileSync('.netlify-reports/performance-trends.json', 'utf8'));\n92:               console.log(Math.round((data.trends.build_performance?.average_duration || 0) / 1000));\n93:             \")\n94:             TOTAL_ERRORS=$(node -e \"\n95:               const data = JSON.parse(require('fs').readFileSync('.netlify-reports/performance-trends.json', 'utf8'));\n96:               console.log(data.trends.error_analysis?.total_errors || 0);\n97:             \")\n98:             RECOMMENDATIONS=$(node -e \"\n99:               const data = JSON.parse(require('fs').readFileSync('.netlify-reports/performance-trends.json', 'utf8'));\n100:               console.log((data.trends.performance_recommendations || []).length);\n101:             \")\n102: \n103:             echo \"health_score=$HEALTH_SCORE\" >> $GITHUB_OUTPUT\n104:             echo \"success_rate=$SUCCESS_RATE\" >> $GITHUB_OUTPUT\n105:             echo \"avg_build_time=$AVG_BUILD_TIME\" >> $GITHUB_OUTPUT\n106:             echo \"total_errors=$TOTAL_ERRORS\" >> $GITHUB_OUTPUT\n107:             echo \"recommendations=$RECOMMENDATIONS\" >> $GITHUB_OUTPUT\n108:           else\n109:             echo \"health_score=0\" >> $GITHUB_OUTPUT\n110:             echo \"success_rate=0\" >> $GITHUB_OUTPUT\n111:             echo \"avg_build_time=0\" >> $GITHUB_OUTPUT\n112:             echo \"total_errors=0\" >> $GITHUB_OUTPUT\n113:             echo \"recommendations=0\" >> $GITHUB_OUTPUT\n114:           fi\n115: \n116:       - name: Create issue for critical findings\n117:         if: |\n118:           steps.performance-summary.outputs.health_score < 80 ||\n119:           steps.performance-summary.outputs.total_errors > 3 ||\n120:           (github.event.inputs.create_issue == 'true' && steps.performance-summary.outputs.recommendations > 0)\n121:         uses: actions/github-script@v7\n122:         with:\n123:           script: |\n124:             const healthScore = ${{ steps.performance-summary.outputs.health_score }};\n125:             const successRate = ${{ steps.performance-summary.outputs.success_rate }};\n126:             const avgBuildTime = ${{ steps.performance-summary.outputs.avg_build_time }};\n127:             const totalErrors = ${{ steps.performance-summary.outputs.total_errors }};\n128:             const recommendations = ${{ steps.performance-summary.outputs.recommendations }};\n129: \n130:             let priority = 'low';\n131:             let emoji = 'ℹ️';\n132: \n133:             if (healthScore < 60) {\n134:               priority = 'high';\n135:               emoji = '🚨';\n136:             } else if (healthScore < 80 || totalErrors > 3) {\n137:               priority = 'medium';\n138:               emoji = '⚠️';\n139:             }\n140: \n141:             const title = `${emoji} Netlify Performance Alert - Health Score: ${healthScore}/100`;\n142: \n143:             let body = `# Netlify Performance Analysis Results\n144: \n145:             **Analysis Date:** ${new Date().toLocaleDateString()}\n146:             **Analysis Type:** ${{ github.event.inputs.analysis_type || 'weekly' }}\n147:             **Priority:** ${priority.toUpperCase()}\n148: \n149:             ## 📊 Key Metrics\n150: \n151:             | Metric | Value | Status |\n152:             |--------|--------|--------|\n153:             | Health Score | ${healthScore}/100 | ${healthScore >= 80 ? '✅ Good' : healthScore >= 60 ? '⚠️ Fair' : '❌ Poor'} |\n154:             | Success Rate | ${successRate}% | ${successRate >= 95 ? '✅ Good' : successRate >= 90 ? '⚠️ Fair' : '❌ Poor'} |\n155:             | Average Build Time | ${avgBuildTime}s | ${avgBuildTime <= 120 ? '✅ Good' : avgBuildTime <= 180 ? '⚠️ Fair' : '❌ Poor'} |\n156:             | Total Errors | ${totalErrors} | ${totalErrors === 0 ? '✅ Good' : totalErrors <= 2 ? '⚠️ Fair' : '❌ Poor'} |\n157:             | Recommendations | ${recommendations} | ${recommendations === 0 ? '✅ Good' : recommendations <= 3 ? '⚠️ Review' : '❌ Action Needed'} |\n158: \n159:             `;\n160: \n161:             if (healthScore < 80) {\n162:               body += `\n163:               ## 🚨 Critical Issues\n164: \n16