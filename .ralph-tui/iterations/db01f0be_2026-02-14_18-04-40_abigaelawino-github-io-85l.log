thod: 'POST',\n272:         headers: { 'Content-Type': 'application/json' },\n273:         body: JSON.stringify(slackMessage),\n274:       });\n275:     } catch (error) {\n276:       console.error('Failed to send success notification:', error);\n277:     }\n278:   }\n279: }\n280: \n281: // Update deployment metrics\n282: function updateDeploymentMetrics(deployment) {\n283:   // In production, you would store these in a database or metrics system\n284:   const metrics = {\n285:     timestamp: new Date().toISOString(),\n286:     deploymentId: deployment.deploymentId,\n287:     repository: deployment.repository,\n288:     branch: deployment.branch,\n289:     status: deployment.status,\n290:     buildTime: deployment.buildTime,\n291:     environment: deployment.environment,\n292:   };\n293: \n294:   console.log('Deployment metrics:', metrics);\n295: }\n296: \n297: // Helper functions for external access\n298: exports.getDeploymentStore = function () {\n299:   return deploymentStore;\n300: };\n301: \n302: exports.getDeploymentById = function (deploymentId) {\n303:   return deploymentStore.get(deploymentId);\n304: };\n305: \n\n(End of file - total 305 lines)\n</content>","title":"netlify/functions/deployment-monitoring.js","metadata":{"preview":"// Deployment monitoring and status tracking\nconst deploymentStore = new Map();\n\n// Main handler function for deployment status updates\nexports.handler = async function (event, context) {\n  try {\n    const { httpMethod } = event;\n\n    switch (httpMethod) {\n      case 'GET':\n        return await getDeploymentStatus(event);\n      case 'POST':\n        return await updateDeploymentStatus(event);\n      case 'DELETE':\n        return await clearDeploymentHistory(event);\n      default:\n        return {\n          statusCode: 405,\n          body: JSON.stringify({ error: 'Method not allowed' }),\n        };","truncated":false,"loaded":[]},"time":{"start":1771110310621,"end":1771110310628}}}}
