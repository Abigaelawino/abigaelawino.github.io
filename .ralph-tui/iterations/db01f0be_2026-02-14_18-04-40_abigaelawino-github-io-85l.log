/ Filter deployments by repository and/or branch\n45:   let deployments = Array.from(deploymentStore.values());\n46: \n47:   if (repository) {\n48:     deployments = deployments.filter(d => d.repository === repository);\n49:   }\n50: \n51:   if (branch) {\n52:     deployments = deployments.filter(d => d.branch === branch);\n53:   }\n54: \n55:   // Sort by timestamp (most recent first)\n56:   deployments.sort((a, b) => new Date(b.timestamp) - new Date(a.timestamp));\n57: \n58:   // Return last 20 deployments if no specific filter\n59:   if (!deploymentId && !repository && !branch) {\n60:     deployments = deployments.slice(0, 20);\n61:   }\n62: \n63:   return {\n64:     statusCode: 200,\n65:     body: JSON.stringify({\n66:       deployments,\n67:       total: deployments.length,\n68:       timestamp: new Date().toISOString(),\n69:     }),\n70:   };\n71: }\n72: \n73: // Update deployment status\n74: async function updateDeploymentStatus(event) {\n75:   const { body } = event;\n76: \n77:   if (!body) {\n78:     return {\n79:       statusCode: 400,\n80:       body: JSON.stringify({ error: 'Missing request body' }),\n81:     };\n82:   }\n83: \n84:   try {\n85:     const deploymentData = JSON.parse(body);\n86: \n87:     // Validate required fields\n88:     const requiredFields = ['deploymentId', 'repository', 'branch', 'status'];\n89:     const missingFields = requiredFields.filter(field => !deploymentData[field]);\n90: \n91:     if (missingFields.length > 0) {\n92:       return {\n93:         statusCode: 400,\n94:         body: JSON.stringify({ error: `Missing required fields: ${missingFields.join(', ')}` }),\n95:       };\n96:     }\n97: \n98:     // Create or update deployment record\n99:     const deployment = {\n100:       deploymentId: deploymentData.deploymentId,\n101:       repository: deploymentData.repository,\n102:       branch: deploymentData.branch,\n103:       status: deploymentData.status, // pending, building, success, failed, cancelled\n104:       timesta