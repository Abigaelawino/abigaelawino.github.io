/ Filter deployments by repository and/or branch\n45:   let deployments = Array.from(deploymentStore.values());\n46: \n47:   if (repository) {\n48:     deployments = deployments.filter(d => d.repository === repository);\n49:   }\n50: \n51:   if (branch) {\n52:     deployments = deployments.filter(d => d.branch === branch);\n53:   }\n54: \n55:   // Sort by timestamp (most recent first)\n56:   deployments.sort((a, b) => new Date(b.timestamp) - new Date(a.timestamp));\n57: \n58:   // Return last 20 deployments if no specific filter\n59:   if (!deploymentId && !repository && !branch) {\n60:     deployments = deployments.slice(0, 20);\n61:   }\n62: \n63:   return {\n64:     statusCode: 200,\n65:     body: JSON.stringify({\n66:       deployments,\n67:       total: deployments.length,\n68:       timestamp: new Date().toISOString(),\n69:     }),\n70:   };\n71: }\n72: \n73: // Update deployment status\n74: async function updateDeploymentStatus(event) {\n75:   const { body } = event;\n76: \n77:   if (!body) {\n78:     return {\n79:       statusCode: 400,\n80:       body: JSON.stringify({ error: 'Missing request body' }),\n81:     };\n82:   }\n83: \n84:   try {\n85:     const deploymentData = JSON.parse(body);\n86: \n87:     // Validate required fields\n88:     const requiredFields = ['deploymentId', 'repository', 'branch', 'status'];\n89:     const missingFields = requiredFields.filter(field => !deploymentData[field]);\n90: \n91:     if (missingFields.length > 0) {\n92:       return {\n93:         statusCode: 400,\n94:         body: JSON.stringify({ error: `Missing required fields: ${missingFields.join(', ')}` }),\n95:       };\n96:     }\n97: \n98:     // Create or update deployment record\n99:     const deployment = {\n100:       deploymentId: deploymentData.deploymentId,\n101:       repository: deploymentData.repository,\n102:       branch: deploymentData.branch,\n103:       status: deploymentData.status, // pending, building, success, failed, cancelled\n104:       timestamp: new Date().toISOString(),\n105:       buildUrl: deploymentData.buildUrl,\n106:       commitSha: deploymentData.commitSha,\n107:       commitMessage: deploymentData.commitMessage,\n108:       author: deploymentData.author,\n109:       deployUrl: deploymentData.deployUrl,\n110:       buildTime: deploymentData.buildTime,\n111:       error: deploymentData.error,\n112:       environment: deploymentData.environment || 'production',\n113:     };\n114: \n115:     // Store deployment\n116:     deploymentStore.set(deployment.deploymentId, deployment);\n117: \n118:     // Trigger notifications for deployment status changes\n119:     await handleDeploymentNotifications(deployment);\n120: \n121:     // Update deployment metrics\n122:     updateDeploymentMetrics(deployment);\n123: \n124:     console.log('Deployment status updated:', deployment);\n125: \n126:     return {\n127:       statusCode: 200,\n128:       body: JSON.stringify({\n129:         message: 'Deployment status updated successfully',\n130:         deployment,\n131:       }),\n132:     };\n133:   } catch (parseError) {\n134:     return {\n135:       statusCode: 400,\n136:       body: JSON.stringify({ error: 'Invalid JSON body' }),\n137:     };\n138:   }\n139: }\n140: \n141: // Clear deployment history\n142: async function clearDeploymentHistory(event) {\n143:   const { queryStringParameters } = event;\n144:   const { olderThan } = queryStringParameters || {};\n145: \n146:   if (olderThan) {\n147:     // Clear deployments older than specified date\n148:     const cutoffDate = new Date(olderThan);\n149:     let clearedCount = 0;\n150: \n151:     for (const [id, deployment] of deploymentStore.entries()) {\n152:       if (new Date(deployment.timestamp) < cutoffDate) {\n153:         deploymentStore.delete(id);\n154:         clearedCount++;\n155:       }\n156:     }\n157: \n158:     return {\n159:       statusCode: 200,\n160:       body: JSON.stringify({\n161:         message: `Cleared ${clearedCount} deployments older than ${olderThan}`,\n162:         clearedCount,\n163:       }),\n164:     };\n165:   } else {\n166:     // Clear all deployments (with confirmation)\n167:     const count = deploymentStore.size;\n168:     deploymentStore.clear();\n169: \n170:     return {\n171:       statusCode: 200,\n172:       body: JSON.stringify({\n173:         message: `Cleared all ${count} deployments`,\n174:         clearedCount: count,\n175:       }),\n176:     };\n177:   }\n178: }\n179: \n180: // Handle deployment notifications\n181: async function handleDeploymentNotifications(deployment) {\n182:   // Send notification for failed deployments\n183:   if (deployment.status === 'failed') {\n184:     await sendFailureNotification(deployment);\n185:   }\n186: \n187:   // Send notification for successful deployments\n188:   if (deployment.status === 'success') {\n189:     await sendSuccessNotification(deployment);\n190:   }\n191: }\n192: \n193: // Send failure notification\n194: async function sendFailureNotification(deployment) {\n195:   console.log('Deployment failed:', deployment);\n196: \n197:   if (process.env.SLACK_WEBHOOK_URL) {\n198:     try {\n199:       const slackMessage = {\n200:         text: `Deployment Failed: ${deployment.repository}:${deployment.branch}`,\n201:         attachments: [\n202:           {\n203:             color: 'danger',\n204:             fields: [\n205:               { title: 'Repository', value: deployment.repository, short: true },\n206:               { title: 'Branch', value: deployment.branch, short: true },\n207:               {\n208:                 title: 'Commit',\n209:                 value: deployment.commitSha?.substring(0, 7) || 'Unknown',\n210:                 short: true,\n211:               },\n212:               { title: 'Author', value: deployment.author || 'Unknown', short: true },\n213:               { title: 'Error', value: deployment.error || 'Unknown error', short: false },\n214:             ],\n215:             actions: [\n216:            