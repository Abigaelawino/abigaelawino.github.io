{
  "name": "abigaelawino-portfolio",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:static": "node scripts/dev.mjs",
    "start": "node scripts/dev-server.mjs",
    "lint": "node scripts/lint.mjs && node scripts/eslint.mjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security": "node scripts/security-check.mjs",
    "typecheck": "node scripts/typecheck-proper.mjs",
    "test": "node --test",
    "smoke-test": "node scripts/smoke-test.mjs",
    "smoke-test:prod": "node scripts/smoke-test.mjs https://abigael-awino-portfolio.netlify.app",
    "smoke-test:quick": "node scripts/smoke-test.mjs https://abigael-awino-portfolio.netlify.app --quick",
    "smoke-test:local": "node scripts/smoke-test-local.mjs",
    "setup:local": "node scripts/setup-local.mjs",
    "generate:content": "node scripts/generate-content-index.mjs",
    "build": "node scripts/generate-content-index.mjs && next build",
    "build:full": "NODE_ENV=production node scripts/validate-env.mjs production && node scripts/build.mjs && NODE_ENV=production next build",
    "build:local": "node scripts/generate-content-index.mjs && node scripts/build.mjs && next build",
    "coverage": "node scripts/run-coverage.mjs && node scripts/check-coverage.mjs",
    "shadcn:coverage": "node scripts/shadcn-coverage.mjs",
    "deploy": "npm run build && netlify deploy --prod --dir=dist",
    "mcp-monitor": "node scripts/mcp-monitor.mjs",
    "mcp-dashboard": "node scripts/mcp-dashboard.mjs",
    "ralph:monitor": "node scripts/ralph-queue-monitor.mjs",
    "ralph:dashboard": "node scripts/ralph-queue-dashboard.mjs",
    "ralph:aging": "node scripts/ralph-bead-aging.mjs",
    "ralph:monitoring": "npm run ralph:monitor check && npm run ralph:aging analyze && npm run ralph:dashboard",
    "netlify:analyze": "node scripts/netlify-log-analyzer.mjs",
    "netlify:track": "node scripts/netlify-performance-tracker.mjs",
    "netlify:trends": "node scripts/netlify-monthly-trends.mjs",
    "netlify:optimize": "node scripts/netlify-optimization-detector.mjs",
    "netlify:dashboard": "node scripts/netlify-dashboard.mjs",
    "netlify:test": "node --test test/netlify-monitoring.test.js",
    "netlify:monitor": "npm run netlify:analyze && npm run netlify:track && npm run netlify:optimize && npm run netlify:dashboard",
    "function:health": "node scripts/netlify-function-health-monitor.mjs",
    "function:dashboard": "node scripts/netlify-function-health-dashboard.mjs",
    "function:alerting": "node scripts/netlify-function-health-alerting.mjs",
    "function:monitor": "npm run function:health && npm run function:alerting",
    "content:freshness": "node scripts/content-freshness-monitor.mjs",
    "content:trends": "node scripts/content-monthly-trends.mjs",
    "content:recommendations": "node scripts/content-recommendations.mjs",
    "content:quarterly": "node scripts/content-quarterly-schedule.mjs",
    "content:health": "node scripts/content-health-validator.mjs",
    "content:monitor": "npm run content:freshness && npm run content:trends && npm run content:recommendations && npm run content:quarterly && npm run content:health",
    "seasonal:plan": "node scripts/seasonal-content-planner.mjs",
    "seasonal:reminders": "node scripts/seasonal-content-reminder.mjs",
    "seasonal:dashboard": "node scripts/seasonal-content-dashboard.mjs",
    "seasonal:templates": "node scripts/seasonal-content-templates.mjs",
    "seasonal:quarterly": "npm run seasonal:plan quarterly && npm run seasonal:templates generate && npm run seasonal:reminders issues",
    "seasonal:annual": "npm run seasonal:plan annual && npm run seasonal:templates generate && npm run seasonal:dashboard dashboard",
    "seasonal:all": "npm run seasonal:plan && npm run seasonal:templates && npm run seasonal:reminders && npm run seasonal:dashboard",
    "lighthouse:track": "node scripts/lighthouse-tracker.mjs",
    "lighthouse:vitals": "node scripts/lighthouse-core-vitals.mjs",
    "lighthouse:recommendations": "node scripts/lighthouse-monthly-recommendations.mjs",
    "lighthouse:alerts": "node scripts/lighthouse-alert-processor.mjs",
    "lighthouse:monitor": "npm run lighthouse:track && npm run lighthouse:vitals && npm run lighthouse:recommendations && npm run lighthouse:alerts",
    "ci": "npm run lint && npm run format:check && npm run security && node scripts/validate-env.mjs production && npm run typecheck && node --test test/ci-comprehensive.test.js && npm run build && npm run coverage",
    "ci:full": "npm run ci && npm run smoke-test:quick"
  },
  "devDependencies": {
    "@netlify/mcp": "^1.15.1",
    "@netlify/plugin-nextjs": "^5.15.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@types/d3-color": "^3.1.3",
    "@types/d3-path": "^3.1.1",
    "@types/ms": "^2.1.0",
    "@types/node": "^25.2.3",
    "@types/node-fetch": "^2.6.13",
    "@types/plotly.js": "^2.35.6",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "canvas": "^3.2.1",
    "chokidar": "^5.0.0",
    "jsdom": "^28.1.0",
    "lighthouse": "^13.0.3",
    "pg-protocol": "^1.11.0",
    "prettier": "^3.8.1",
    "shadcn": "^3.8.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.24",
    "chrome-launcher": "^1.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.564.0",
    "next": "^16.1.6",
    "next-mdx-remote": "^6.0.0",
    "plotly.js": "^3.3.1",
    "plotly.js-dist-min": "^3.3.1",
    "postcss": "^8.5.6",
    "qs": "^6.15.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-is": "^19.2.0",
    "react-plotly.js": "^2.6.0",
    "reading-time": "^1.5.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  }
}
